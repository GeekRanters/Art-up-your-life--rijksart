<div class="resultCanvas">
  <h1><%= @title %></h1>
  <h2>Artist: <%= @principalMaker %></h2>

  <div class="detailimageresult">
    <a href=<%= @webImage %>>
      <img class="detailimage" src=<%= @webImage %> alt="Small size artwork">
    </a>
  </div>

  <div class="collections_box">
    <button type="button" name="shirt">T-Shirt</button>
    <button type="button" name="stubby">Stubby Holder</button>
    <button type="button" name="print">Art Print</button>
    <button type="button" name="favourite">Favourite</button>
  </div>

  <div class="detaildescription">
  <p>Description: <%= @label %></p>
  </div>
</div>
